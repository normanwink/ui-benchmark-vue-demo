{
  "name": "ui-benchmark-vue-demo",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "concurrently \"npm run dev\" \"npm run server\"",
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
